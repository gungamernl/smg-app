<Window x:Class="SMG_app.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SMG_app"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        
        <Grid Margin="30,30,30,30">
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                <Button  Width="200" Margin="0,10" Height="40" Click="nummers_click">nummers</Button>
                <Button  Width="200" Margin="0,10" Height="40" Click="gebruikers_Click">gebruikers</Button>
                <Button  Width="200" Margin="0,10" Height="40" Click="artiesten_Click">artiesten</Button>

            </StackPanel>
        </Grid>
        
    </Grid>
</Window>
